{
  "name": "nginclient",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings=0 .",
    "lintfix": "eslint . --max-warnings=0 --fix",
    "test": "vitest --watch=false",
    "coverage": "vitest run --coverage",
    "buildrun": "tsc -b && vite build && vite",
    "beforepush": "eslint --max-warnings=0 && tsc -b && vite build && vitest --watch=false"
  },
  "dependencies": {
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  }
}
